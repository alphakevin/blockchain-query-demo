{
  "name": "blockchain-query-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src api",
    "clean": "rm -rf build",
    "start": "npm-run-all --parallel start:*",
    "start:client": "NODE_ENV=development react-app-rewired start",
    "start:server": "nodemon api/index.js -w api",
    "bundle": "yarn clean && npm run bundle:client && npm run bundle:server",
    "bundle:client": "PUBLIC_URL=\"/.blocklet/proxy/z8iZueEP9y4RX4LTQkwUe27ASe74L9riRWV2Q\" REACT_APP_API_PREFIX=\"\" REACT_APP_APP_NAME=\"Blockchain Query Demo\" yarn build:client",
    "bundle:server": "blocklet bundle --create-release",
    "build:client": "react-scripts build && rm -f build/**/**/*.{js,css}.map",
    "test": "node tools/jest.js",
    "release": "npm run clean && npm run bundle",
    "coverage": "npm run test -- --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@abtnode/nedb": "^1.0.32",
    "@apollo/client": "^3.3.21",
    "@arcblock/did-react": "^1.14.10",
    "@arcblock/ux": "^1.14.10",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-history-api-fallback": "^2.2.1",
    "graphql": "^15.5.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "http-proxy-middleware": "^2.0.1",
    "jest": "26.6.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.8"
  }
}
